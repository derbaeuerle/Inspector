<?xml version="1.0" encoding="utf-8"?>
<Inspector>

    <Gadget>
        <url-pattern>/get/integer/\d{1,4}</url-pattern>
        <handler>de.hsrm.inspector.web.handler.IntegerHandler</handler>
    </Gadget>

    <Gadget>
        <url-pattern>/audio</url-pattern>
        <handler>de.hsrm.inspector.web.handler.AudioHandler</handler>
        <uses-service>de.hsrm.inspector.services.AudioService</uses-service>
    </Gadget>
    
</Inspector>