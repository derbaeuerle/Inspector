<?xml version="1.0" encoding="utf-8"?>
<Inspector timeout="60">

    <Gadget>
        <identifier>AUDIO</identifier>
        <class>de.hsrm.inspector.gadgets.AudioGadget</class>
        <preference>audio_preferences</preference>
    </Gadget>

    <Gadget>
        <identifiers>
            <identifier>ACCELERATION</identifier>
            <identifier>GYROSCOPE</identifier>
            <identifier>TEMPERATURE</identifier>
            <identifier>LIGHT</identifier>
            <identifier>MAGNETIC</identifier>
            <identifier>ORIENTATION</identifier>
            <identifier>PRESSURE</identifier>
            <identifier>PROXIMITY</identifier>
        </identifiers>
        <class>de.hsrm.inspector.gadgets.SensorGadget</class>
        <preference>sensor_preferences</preference>
    </Gadget>

</Inspector>